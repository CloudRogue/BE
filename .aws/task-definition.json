{
  "family": "zipchak-auth-task",
  "containerDefinitions": [
    {
      "name": "auth-container",
      "image": "514994861854.dkr.ecr.ap-northeast-2.amazonaws.com/zipchak/kakao:latest",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "KAKAO_REDIRECT_URI",
          "value": "http://43.202.161.219/login/oauth2/code/kakao"
        },
        {
          "name": "KAKAO_CLIENT_ID",
          "value": "KAKAO_CLIENT_ID_VALUE"
        }
      ]
    }
  ],
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "512",
  "memory": "1024"
}